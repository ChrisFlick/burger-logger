<div class="wrapper">
    <h1 class="jumbotron text-each">Burgers!</h1>
    <div class='row'>
        
        <div class="col-md-5">
            Burgers:

            <ul>
            {{#unless eaten}}
                <li>{{burger_name}}<button class="devour">Devour!</button></li><br>
            {{/unless}}
            </ul>
        </div>
        <div class="col-md-5">
            Eaten burgers:
        </div>
    </div>
    <form id="new burger">
        <textarea class="" id="burger"></textarea>
        <input type="submit">
    </form>
</div>